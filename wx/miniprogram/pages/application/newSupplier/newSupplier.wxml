<cu-custom bgColor="bg-gradual-pink" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">新增供应商</view>
</cu-custom>
<view class="cu-form-group margin-top">
  <view class="title">供应商名称 *</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">供应商类型 *</view>
  <picker bindchange="typeChange" value="{{tindex}}" range="{{type}}">
    <view class="picker">
      {{tindex?type[tindex]:'请选择'}}
    </view>
  </picker>
</view>
<view class="cu-form-group margin-top">
  <view class="title">增值税税率</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">纳税人识别号</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">开户银行</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">银行账户</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>

<view class="cu-form-group margin-top">
  <view class="title">联系人</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">手机</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">公司座机</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">详细地址</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group margin-top">
  <view class="title">期初应收款</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>
<view class="cu-form-group">
  <view class="title">期初预收款</view>
  <input placeholder="请输入" style="text-align:right"></input>
</view>

<view class="padding flex flex-direction">
  <button class="cu-btn bg-red margin-tb-sm lg" bindtap='addsuccess'>确定</button>
   <button class="cu-btn bg-grey lg" bindtap='addfail'>取消</button>
</view>
